<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="CENTER" hgap="10" prefHeight="220.0"
	prefWidth="320.0" styleClass="root" vgap="10" xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.albom.gui.MainWindowController">
	<children>

		<Label text="DateTime:" GridPane.columnIndex="0"
			GridPane.rowIndex="0" />
		<Label text="DateTime (UTC):" GridPane.rowIndex="1" />
		<Label text="Timestamp:" GridPane.rowIndex="2" />
		<Label text="Day of year:" GridPane.rowIndex="3" />
		<Label text="GPS week:" GridPane.rowIndex="4" />

		<TextField fx:id="textDateTime" editable="false" minWidth="200.0"
			prefWidth="200.0" GridPane.rowIndex="0" GridPane.columnIndex="1" />
		<TextField fx:id="textDateTimeUtc" editable="false" GridPane.columnIndex="1"
			GridPane.rowIndex="1" />
		<TextField fx:id="textTimestamp" editable="false"
			GridPane.columnIndex="1" GridPane.rowIndex="2" />
		<TextField fx:id="textDayOfYear" editable="false" GridPane.columnIndex="1"
			GridPane.rowIndex="3" />
		<TextField fx:id="textGpsWeek" editable="false" GridPane.columnIndex="1"
			GridPane.rowIndex="4" />

	</children>
	<columnConstraints>
		<ColumnConstraints />
		<ColumnConstraints />
	</columnConstraints>
	<rowConstraints>
		<RowConstraints />
		<RowConstraints />
		<RowConstraints />
		<RowConstraints />
		<RowConstraints />
	</rowConstraints>
	<opaqueInsets>
		<Insets />
	</opaqueInsets>
	<padding>
		<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
	</padding>

</GridPane>
